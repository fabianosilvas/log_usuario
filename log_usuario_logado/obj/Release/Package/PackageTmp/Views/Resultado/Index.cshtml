@model IEnumerable<log_usuario_logado.Models.reatb001_resultado>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VR_OBJETIVO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VR_REALIZADO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PC_REALIZADO) <br />

        </th>
      
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VR_OBJETIVO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VR_REALIZADO)
        </td>

        <td>
            <span style="width:40px;" class="badge @item.CorPercentualRealize(item.PC_REALIZADO ?? 0, true) " >  @Html.DisplayFor(modelItem => item.PC_REALIZADO) </span>
        </td>
     
    </tr>
    }

</table>
